{
    "name": "deawx/easycore",
    "description": "Cyberthai Easycore Fast PHP API Framework",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "App/",
            "Core\\": "Core/"
        }
    },
    "keywords": [
        "deawx",
        "easycore",
        "mvc php",
        "PHP",
        "API",
        "Framework",
        "Cyberthai",
        "Easycore",
        "Fast",
        "Backend",
        "Web Development",
        "RESTful",
        "Microservices"
    ],
    "support": {
        "issues": "https://github.com/deawx/easycore/issues",
        "source": "https://github.com/deawx/easycore"
    },
    "homepage": "https://cyberthai.net",
    "authors": [
        {
            "name": "(deawx) Tirapong Chaiyakun",
            "email": "msdos43@gmail.com",
            "homepage": "https://cyberthai.net"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "catfan/medoo": "^2.1",
        "filp/whoops": "^2.15",
        "firebase/php-jwt": "^6.10",
        "vlucas/phpdotenv": "^5.6"
    },
    "scripts": {
        "start": "php -S localhost:8000 -t ."
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}